{
    "version": "2.0.0a1",
    "name": "aiida_flipper",
    "url": "",
    "license": "MIT License",
    "author": "Leonid Kahle",
    "author_email": "leonid.kahle@epfl.ch",
    "description": "For flipper",
    "classifiers": [
        "License :: OSI Approved :: MIT License",
        "Programming Language :: Python :: 2.7",
        "Programming Language :: Python :: 3.7",
        "Development Status :: 4 - Beta"
    ],
    "install_requires": [
        "samos>=0.3.1",
        "aiida_core[atomic_tools]>=1.0.0b1"
    ],
    "entry_points": {
        "aiida.calculations": [
            "quantumespresso.flipper = aiida_flipper.calculations.flipper:FlipperCalculation",
            "quantumespresso.pes = aiida_flipper.calculations.pes:PesCalculation",
            "quantumespresso.hustler = aiida_flipper.calculations.hustler:HustlerCalculation",
            "chillstep.branching=aiida_flipper.calculations.branching.BranchingCalculation",
            "chillstep.moldyn=aiida_flipper.calculations.moldyn.MoldynCalculation"
         ],
        "aiida.parsers": [
            "quantumespresso.flipper = aiida_flipper.parsers.flipper:FlipperParser",
            "quantumespresso.pes = aiida_flipper.parsers.pes:PesParser"
        ]
    },
    "extras_require": {
        "pre-commit": [
            "pre-commit==1.11.0",
            "yapf==0.24.0",
            "prospector==0.12.11",
            "pylint==1.9.3"
        ]
    }
}
